{
  "presets": [
    "@babel/preset-env",
  ],
  "plugins": [
    [
      "@babel/plugin-transform-runtime",
      {
        "useESModules": true,
        "regenerator": false
      }
    ],
    [
      "@babel/plugin-proposal-class-properties"
    ],
    ["@babel/plugin-transform-react-jsx"]
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": "current node"
        }]
      ]
    }
  }
}
